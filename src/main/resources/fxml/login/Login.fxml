        <TextField fx:id="usernameTextBox" layoutX="126.0" layoutY="38.0" />
        <PasswordField fx:id="passwordTextBox" layoutX="126.0" layoutY="79.0" />
        <Button fx:id="logInButton" layoutX="159.0" layoutY="208.0" mnemonicParsing="false" onAction="#logInButtonPressed" text="Log in" />
